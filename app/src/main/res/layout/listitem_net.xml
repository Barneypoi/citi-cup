<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/textview_border"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:paddingTop="10dp">

    <RelativeLayout
        android:id="@+id/rl_net"
        android:layout_width="220dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/netListFundName"
            android:layout_width="wrap_content"
            android:textColor="#000000"
            android:layout_height="30dp"
            android:text="TextView"
            android:textSize="15sp"
            android:layout_marginLeft="20dp"/>
        <TextView
            android:id="@+id/netListFundId"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_below="@id/netListFundName"
            android:layout_marginLeft="20dp"
            android:text="TextView"
            android:textSize="10sp" />
    </RelativeLayout>


    <TextView
        android:id="@+id/netListFundNetWeight"
        android:layout_width="82dp"
        android:layout_height="40dp"
        android:layout_weight="1"
        android:layout_toRightOf="@id/rl_net"
        android:textColor="#000000"
        android:gravity="center"
        android:text="TextView"
        android:textSize="13sp" />

    <TextView
        android:id="@+id/netListFundIncre"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_weight="1"
        android:layout_toRightOf="@id/netListFundNetWeight"
        android:textColor="#000000"
        android:gravity="center"
        android:layout_marginLeft="5dp"
        android:text="TextView"
        android:textSize="13sp" />
</RelativeLayout>